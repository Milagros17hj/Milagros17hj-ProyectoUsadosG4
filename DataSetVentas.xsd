                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT fe.id_factura,
       fe.fecha AS fecha_venta,
       cli.nombre AS cliente,
       v.marca,
       v.modelo,
       fe.total,
       fd.subtotal,
       fd.iva,
       fd.prima,
       fd.interes,
       fd.seguro,
       fd.meses_plazo,
       fd.cuota_mensual
FROM [BD_USADOSCR].[dbo].[FacturaEncabezado] fe
INNER JOIN [BD_USADOSCR].[dbo].[FacturaDetalle] fd
    ON fe.id_factura = fd.id_factura
INNER JOIN [BD_USADOSCR].[dbo].[Reserva] r
    ON fe.id_reserva = r.id_reserva
INNER JOIN [BD_USADOSCR].[dbo].[Cita] c
    ON r.id_cita = c.id_cita
INNER JOIN [BD_USADOSCR].[dbo].[Cliente] cli
    ON c.id_cliente = cli.id_cliente
INNER JOIN [BD_USADOSCR].[dbo].[Vehiculo] v
    ON r.id_vehiculo = v.id_vehiculo;</CommandText>
